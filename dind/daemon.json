{
  "hosts": [
    "unix:///var/run/docker.sock"
  ],
  "icc": false,
  "iptables": true,
  "ipv6": false,
  "live-restore": false,
  "log-driver": "none",
  "log-opts": {
    "max-size": "100m"
  },
  "max-concurrent-downloads": 20,
  "max-concurrent-uploads": 20,
  "shutdown-timeout": 5,
  "storage-driver": "overlay2",
  "userland-proxy": false
}
